UKM=/data/UKM;
BB=$UKM/busybox;

if [ $1 == "dalvik.vm.heapsize" ]; then
	VMHS="`getprop "$1" 2> /dev/null`";
	VMHS=${VMHS%?};

	$BB echo $VMHS;
else
	if [ "`getprop "$1" 2> /dev/null`" == "" ]; then
		if [ "$1" == "persist.sys.purgeable_assets" ]; then
			$BB echo "0";
		elif [[ "$1" != *".dns"* ]]; then
			$BB echo "-1";
		else
			$BB echo "0.0.0.0";
		fi
	else
		$BB echo "`getprop "$1" 2> /dev/null`";
	fi;
fi;
